{
  "name": "order-payments-module",
  "version": "1.0.0",
  "description": "A React module for handling orders and payments in a service marketplace application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc && babel src --out-dir dist --extensions '.ts,.tsx'",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "orders",
    "payments",
    "marketplace",
    "stripe",
    "firebase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "^5.0.0-alpha.155",
    "@mui/material": "^5.15.0",
    "@stripe/stripe-js": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-router": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/order-payments-module.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/order-payments-module/issues"
